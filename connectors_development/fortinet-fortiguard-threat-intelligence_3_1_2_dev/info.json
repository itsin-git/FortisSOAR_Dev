        {
            "operation": "ingest_feeds",
            "title": "Fetch Threat Intel Feeds",
            "description": "Download the FortiGuard Threat Intel Feeds",
            "enabled": true,
            "category": "investigation",
            "annotation": "threat_intel_feeds",
            "parameters": [
                {
                    "title": "Fetch feeds created after",
                    "description": "Specify the start DateTime of the duration to fetch feeds incrementally since the last successful pull during data ingestion.",
                    "tooltip": "Specify the start DateTime of the duration to fetch feeds incrementally since the last successful pull during data ingestion.",
                    "type": "datetime",
                    "name": "modified_after",
                    "required": false,
                    "editable": true,
                    "visible": true
                },
                {
                    "title": "Process Response As",
                    "name": "output_mode",
                    "visible": true,
                    "required": false,
                    "editable": true,
                    "type": "select",
                    "options": [
                        "Create as Feed Records in FortiSOAR",
                        "Save to File"
                    ],
                    "value": "Create as Feed Records in FortiSOAR",
                    "tooltip": "Select the method of returning the Feed Data information",
                    "description": "Select the method of returning the Feed Data information. You can choose between the data being create as Feed records in FortiSOAR or being written to files on the FortiSOAR server (Save to File). ",
                    "onchange": {
                        "Create as Feed Records in FortiSOAR": [
                            {
                                "title": "Record Creation Playbook IRI",
                                "description": "Specify the IRI of the playbook that creates feed records in FortiSOARTM in the Record Creation Playbook IRI field.",
